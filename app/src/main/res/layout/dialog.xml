<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:padding="10dp">

        <TextView
            android:id="@+id/textViewDialog"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Tillykke! du har vundet ! "
            android:textSize="20dp" />

        <Button
            android:id="@+id/buttonDialogOK"
            android:layout_height="wrap_content"
            android:layout_width="100dp"
            android:layout_alignParentRight="true"
            android:layout_alignParentBottom="true"
            android:text="OK" />

        <Button
            android:id="@+id/buttonDialogBack"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentBottom="true"
            android:text="Tilbage" />

        <EditText
            android:id="@+id/editTextDialogName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="Skriv navn her"
            tools:layout_editor_absoluteX="1dp"
            tools:layout_editor_absoluteY="68dp" />

    </RelativeLayout>


</RelativeLayout>